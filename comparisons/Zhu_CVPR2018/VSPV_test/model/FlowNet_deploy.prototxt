layer {
  name: "gxy"
  type: "Python"
  top: "gxy"
  python_param {
    module: "meshgrid_layer"
    layer: "meshgrid_layer"
    param_str: "{\'height\': 200, \'batch_size\': 1, \'width\': 200}"
  }
}

layer {
  name: "coords"
  type: "Eltwise"
  bottom: "d11"
  bottom: "gxy"
  top: "coords"
  eltwise_param {
    coeff: 1
    coeff: 1
  }
}
layer {
  name: "predImg"
  type: "Remap"
  bottom: "srcImg"
  bottom: "coords"
  top: "predImg"
}

layer {
  name: "imgLoss"
  type: "Python"
  bottom: "predImg"
  bottom: "tgtImg"
  top: "imgLoss"
  python_param {
    module: "FN_maskedL1_layer_2"
    layer: "FN_maskedL1_layer_2"
    param_str: "{\'loss_weight\': 0.00000001}"
  }
  loss_weight: 1
}










