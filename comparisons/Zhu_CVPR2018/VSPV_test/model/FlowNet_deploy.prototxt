name: "RED-Net"
layer {
  name: "forward_flow"
  type: "DummyData"
  top: "forward_flow"
  dummy_data_param {
    shape {
      dim: 1
      dim: 2
      dim: 200
      dim: 200
    }
  }
}

layer {
  name: "srcImg"
  type: "DummyData"
  top: "srcImg"
  dummy_data_param {
    shape {
      dim: 1
      dim: 3
      dim: 200
      dim: 200
    }
  }
}

layer {
  name: "tgtImg"
  type: "DummyData"
  top: "tgtImg"
  dummy_data_param {
    shape {
      dim: 1
      dim: 3
      dim: 200
      dim: 200
    }
  }
}

layer {
  name: "gt_xyz"
  type: "DummyData"
  top: "gt_xyz"
  dummy_data_param {
    shape {
      dim: 1
      dim: 3
      dim: 200
      dim: 200
    }
  }
}

layer {
  name: "tform"
  type: "DummyData"
  top: "tform"
  dummy_data_param {
    shape {
      dim: 1
    }
  }
}

layer {
  name: "tgtMask"
  type: "DummyData"
  top: "tgtMask"
  dummy_data_param {
    shape {
      dim: 1
      dim: 1
      dim: 200
      dim: 200
    }
  }
}


layer {
  name: "forward_backward_remap"
  type: "Python"
  bottom: "forward_flow"
  bottom: "gt_xyz"
  bottom: "tform"
  top: "remaped_forward_flow"
  python_param {
    module: "forward_backward_map_layer"
    layer: "forward_backward_map_layer"
  }
}


# conv6
layer {
  name: "conv6"
  type: "Convolution"
  bottom: "remaped_forward_flow"
  top: "c6"
  convolution_param { weight_filler { type: "xavier" } num_output:  128    kernel_size: 3    stride: 1    pad: 1 }
}
layer {  name: "relu6"  type: "ReLU"  bottom: "c6"  top: "c6" }

# conv7
layer {
  name: "conv7"
  type: "Convolution"
  bottom: "c6"
  top: "c7"
  convolution_param { weight_filler { type: "xavier" } num_output:  128    kernel_size: 3    stride: 1    pad: 1 }
}
layer {  name: "relu7"  type: "ReLU"  bottom: "c7"  top: "c7" }

# conv8
layer {
  name: "conv8"
  type: "Convolution"
  bottom: "c7"
  top: "c8"
  convolution_param { weight_filler { type: "xavier" } num_output:  128    kernel_size: 3    stride: 1    pad: 1 }
}
layer {  name: "relu8"  type: "ReLU"  bottom: "c8"  top: "c8" }

# conv9
layer {
  name: "conv9"
  type: "Convolution"
  bottom: "c8"
  top: "c9"
  convolution_param { weight_filler { type: "xavier" } num_output:  128    kernel_size: 3    stride: 1    pad: 1 }
}
layer {  name: "relu9"  type: "ReLU"  bottom: "c9"  top: "c9" }

