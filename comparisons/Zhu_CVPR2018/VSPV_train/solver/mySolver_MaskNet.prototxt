net: "./model/MaskNet_train.prototxt"

# The parameters below are from paper "View Synthesis by Appearance Flow"

# beta 1
momentum: 0.9

# beta 2
momentum2: 0.999

# initial learning rate
base_lr: 0.0001

# step size
stepsize: 50000

# step multiplier
gamma: 0.5

# snapshot intermediate results
snapshot: 10000
snapshot_prefix: "./snapshot/MN/iter"

# solver mode: CPU or GPU
type: "Adam"
solver_mode: GPU

lr_policy: 'step'
